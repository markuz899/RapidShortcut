{
  "manifest_version": 2,

  "name": "Record",
  "description": "Record your token",
  "version": "1.0",
  "author": "Markuz",
  "browser_action": {
    "default_icon": "icon.png",
    "default_popup": "index.html",
    "default_title": "Rapid copy"
  },
  "permissions": [
    "activeTab",
    "unlimitedStorage",
    "clipboardRead",
    "clipboardWrite",
    "storage"
  ]
}
